@model Rocky.Models.Order

<div class="text-center">
    @if (Model != null)
    {
        <h1 class="text-info">Order Created</h1>    
        <p>Order #@Model.Id has been created successfully!</p>
    }
    else
    {
        <h1 class="text-info">Inquiry Confirmation</h1>
        <p>we have received your inquiry, our sales team will contact you shortly!</p>
    }
    
    <img src="/images/patio.jpg" alt="" width="80%" />
</div>